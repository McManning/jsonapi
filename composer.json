{
    "name": "mcmanning/jsonapi",
    "description": "Expose Propel 2 ORM models as JSON:API endpoints",
    "keywords": [ "propel", "behavior", "jsonapi", "slim" ],
    "type": "propel-behavior",
    "license": "MIT",
    "authors": [
        {
            "name": "Chase McManning",
            "email": "cmcmanning@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^2.5",
        "slim/slim": "3.4.0",
        "propel/propel": "2.0.0-alpha8"
    },
    "autoload": {
        "psr-4": {
            "McManning\\JsonApi\\": "src/"
        }
    },
    "extra": {
        "name": "jsonapi",
        "class": "\\McManning\\JsonApi\\Propel\\Behavior\\JsonApiBehavior"
    }
}
